{
  "name": "Pokeshot Autotranslate",
  "title": "Pokeshot Autotranslate App",
  "description": "Adds Pokeshot Autotranslate support to IBM Connections Homepage",
  "services": [
    "Customizer"
  ],
  "extensions": [
    {
      "payload": {
        "include-files": [
          "helloWorld.user.js"
        ],
        "include-repo":{
          "name": "ibm-connections"
        },
        "cache-headers": {
          "cache-control": "max-age=0"
        }
      },
      "name": "Pokeshot Autotranslate Extension",
      "type": "com.ibm.customizer.ui",
      "path": "global"
    }
  ]
}